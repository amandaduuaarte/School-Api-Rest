// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Students {
  id        Int    @id @default(autoincrement())
  name      String
  last_name String
  email     String
  height    Float
  width     Float
}
